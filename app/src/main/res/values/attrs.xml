<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PullToRefresh">
        <attr name="adapterViewBackground" format="reference|color" />
        <attr name="headerBackground" format="reference|color" />
        <attr name="headerTextColor" format="color" />
        <attr name="mode">
            <flag name="pullDownFromTop" value="0x1" />
            <flag name="pullUpFromBottom" value="0x2" />
            <flag name="both" value="0x3" />
        </attr>
        <attr name="loadingView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="errorView" format="reference" />
    </declare-styleable>
    <declare-styleable name="swipelistviewstyle">
        <attr name="isCanSwipe" format="boolean" />
        <attr name="right_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="WidgetSettingItem">
        <attr name="wsiLeftIcon" format="reference" />
        <attr name="wsiLeftText" format="string" />
        <attr name="wsiRightIcon" format="reference" />
        <attr name="wsiRightText" format="string" />
        <attr name="wsiRightTextIcon" format="reference" />
        <attr name="wsiShowbottomLine" format="boolean" />
    </declare-styleable>
    <declare-styleable name="LineTextView">
        <attr name="linet" format="boolean" />
        <attr name="linec" format="boolean" />
        <attr name="linee" format="boolean" />
        <attr name="normaltitle" format="string" />
        <attr name="showmore" format="boolean" />
    </declare-styleable>
    <declare-styleable name="MutiLineTextView">
        <attr name="mutilinet" format="boolean" />
        <attr name="mutilinec" format="boolean" />
        <attr name="mutilinee" format="boolean" />
        <attr name="mutinormaltitle" format="string" />
        <attr name="mutishowmore" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="circle_textview">
        <attr name="circlebackground" format="reference|color"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="integer" />
    </declare-styleable>
</resources>